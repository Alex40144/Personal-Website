datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["mongodb"]
}

model Users {
  name       String   
  id         String      @id @default(dbgenerated()) @map("_id")
  email      String   @unique 
  password   String?  
  categories String[]
  data       Data[]
}

model Data {
  title     String
  value     Float
  frequency String
  id        String     @unique @default(dbgenerated())
  category  String
  users Users   @relation(fields: [userId], references: [id])
  userId String
}
