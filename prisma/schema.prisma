datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongodb"]
}

model Users {
  name       String
  id         String   @default(dbgenerated()) @map("_id") @id @db.ObjectId
  email      String   @unique
  password   String
  settings Json
  data       Data[]
}

model Data {
  title     String
  value     Float
  frequency String
  id        String @unique @default(dbgenerated()) @db.ObjectId @id @map("_id")
  category  String
  users     Users  @relation(fields: [ownerId], references: [id])
  ownerId   String @db.ObjectId
}
